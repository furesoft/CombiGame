             xmlns:local="using:CombiGame"
             x:Class="CombiGame.App" x:CompileBindings="False"
             RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />

        <StyleInclude Source="avares://NodeEditorAvalonia/Themes/NodeEditorTheme.axaml" />
    </Application.Styles>
</Application>